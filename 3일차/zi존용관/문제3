import turtle
a=turtle.Pen()
a.color('red')
a.circle(70)
a.left(72)
a.color('purple')
a.circle(70)
a.left(72)
a.color('green')
a.circle(70)
a.left(72)
a.color('blue')
a.circle(70)
a.left(72)
a.color('yellow')
a.circle(70)








































































turtle.exitonclick()